@model TraversalCoreProject.CQRS.Commands.GuideCommands.CreateGuideCommand

@{
    Layout="_AdminLayout";
}

<h2>Yeni Rehber Ekleme Sayfası</h2>
<form method="post">
    <label>Rehber Adı</label>
    <input type="text" class="form-control" asp-for="Name"/>
    <br/>
    <label>Rehber Açıklaması</label>
    <input type="text" class="form-control" asp-for="Description"/>
    <br/>
    <button class="btn btn-outline-info">Kaydet</button>
</form>